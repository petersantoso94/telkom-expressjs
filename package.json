{
  "name": "express-postgres-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Starter project using Express and Postgres",
  "main": "config.js",
  "scripts": {
    "test": "xo",
    "lint": "xo",
    "format": "xo --fix",
    "start": "node ./bin/start.js",
    "dev": "nodemon ./bin/start.js",
    "migrate": "node ./bin/migrate.js",
    "migrate:create": "migrate create --migrations-dir='./src/migrations'"
  },
  "keywords": [
    "express",
    "postgres"
  ],
  "author": "Peter Santoso",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "5.0.0",
    "client-sessions": "^0.8.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "migrate": "^1.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "sql-template-strings": "^2.2.2",
    "swagger-jsdoc": "^6.0.0-rc.3",
    "swagger-ui-express": "^4.1.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "xo": "^0.28.3"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
